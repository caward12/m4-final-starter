<h2>Sign In to Account</h2>

<%= form_for(:session, url: login_path) do |f| %>
<br><br>
  <%= f.label :email %>
  <%= f.text_field :email %>
<br><br>
  <%= f.label :password %>
  <%= f.password_field :password %>
<br><br>
  <%= f.label :password_confirmation %>
  <%= f.password_field :password_confirmation %>
<br><br>
  <%= f.submit "Log In", class: "login" %>

<% end %>