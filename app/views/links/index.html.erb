<% if current_user %>
<%= link_to "Sign Out", logout_path  %>
<% else %>
<%= link_to "Sign Up", new_user_path  %>
<%= link_to "Sign In", login_path %>
<% end %>

<div class="new-link">
  <%= render partial: "form" %>
</div>
<hr>

<%= @links.each do |link| %>
  <h3>Title: <%= link.title %></h3>
  URL: <%= link.url %>
  Read: <%= link.read %>
<% end %>
